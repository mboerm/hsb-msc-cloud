<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<services>
	<!--Compute services-->
	<service name="VM">
		<aws>Amazon EC2</aws>
		<azure>Azure Virtual Machines</azure>
		<gcp>Google Compute Engine</gcp>
	</service>
	<service name="Container (Standard)">
		<aws>Amazon Elastic Container Service</aws>
		<azure>Azure Container Instances</azure>
		<gcp>Google Container Registry</gcp>
	</service>
	<service name="Container (Kubernetes)">
		<aws>Amazon Elastic Kubernetes Service</aws>
		<azure>Azure Kubernetes Service</azure>
		<gcp>Google Kubernetes Engine</gcp>
	</service>
	<service name="App">
		<aws>AWS Elastic Beanstalk</aws>
		<azure>Azure App Engine</azure>
		<gcp>Google App Engine</gcp>
	</service>
	<service name="Batch">
		<aws>AWS Batch</aws>
		<azure>Azure Batch</azure>
		<gcp>Google Batch</gcp>
	</service>
	<service name="Code">
		<aws>AWS Lambda</aws>
		<azure>Azure Functions</azure>
		<gcp>Cloud Functions</gcp>
	</service>
	<service name="Load Balancing">
		<aws>Elastic Load Balancing</aws>
		<azure>Azure Load Balancer</azure>
		<gcp>Cloud Load Balancing</gcp>
	</service>
	
	<!--Storage services-->
	<service name="Object-Storage (Standard)">
		<aws>Amazon Simple Storage Service</aws>
		<azure>Azure Blob Storage</azure>
		<gcp>Cloud Storage</gcp>
	</service>
	<service name="Object-Storage (Archive)">
		<aws>Amazon S3 Glacier</aws>
		<azure>Azure Blob Storage (Cold/Archive-Mode)</azure>
		<gcp>Cloud Storage (Coldline-Mode)</gcp>
	</service>
	<service name="Block-Storage">
		<aws>Amazon Elastic Block Store</aws>
		<azure>Azure Disk Storage</azure>
		<gcp>Persistent Disk</gcp>
	</service>
	<service name="File-Storage">
		<aws>Amazon Elastic File System</aws>
		<azure>Azure File Storage</azure>
		<gcp>Cloud Filestore</gcp>
	</service>
	
	<!--Database services-->
	<service name="SQL (MySQL)">
		<aws>Amazon RDS for MySQL, Amazon Aurora for MySQL</aws>
		<azure>Azure Database for MySQL, Azure SQL Database</azure>
		<gcp>Cloud SQL, Cloud Spanner</gcp>
	</service>
	<service name="SQL (PostgreSQL)">
		<aws>Amazon RDS for PostgreSQL, Amazon Aurora for PostgreSQL</aws>
		<azure>Azure Database for PostgreSQL</azure>
		<gcp>Cloud SQL for PostgreSQL</gcp>
	</service>
	<service name="SQL (MariaDB)">
		<aws>Amazon RDS for MariaDB</aws>
		<azure>Azure Database for MariaDB</azure>
		<gcp>-</gcp>
	</service>
	<service name="NoSQL">
		<aws>Amazon DynamoDB</aws>
		<azure>Azure Table Storage</azure>
		<gcp>Cloud Bigtable</gcp>
	</service>
	<service name="Document">
		<aws>Amazon DocumentDB</aws>
		<azure>Azure Cosmos DB</azure>
		<gcp>Cloud Firestore</gcp>
	</service>
	<service name="Cache">
		<aws>Amazon ElastiCache</aws>
		<azure>Azure Cache for Redis</azure>
		<gcp>Cloud Memorystore</gcp>
	</service>
	
	<!--Analytic services-->
	<service name="Data Query">
		<aws>Amazon Athena</aws>
		<azure>Azure Data Lake Analytics</azure>
		<gcp>BigQuery</gcp>
	</service>
	<service name="Data Share">
		<aws>AWS Data Exchange</aws>
		<azure>Azure Data Share</azure>
		<gcp>Cloud Pub/Sub</gcp>
	</service>
	<service name="Data Transfer">
		<aws>AWS Data Pipeline</aws>
		<azure>Azure Data Factory</azure>
		<gcp>Cloud Composer</gcp>
	</service>
	<service name="Data Catalog">
		<aws>AWS Glue</aws>
		<azure>Azure Data Catalog</azure>
		<gcp>Data Catalog</gcp>
	</service>
	<service name="Data Lake">
		<aws>AWS Lake Formation</aws>
		<azure>Azure Data Lake Storage</azure>
		<gcp>Cloud Dataprep</gcp>
	</service>
	<service name="Search Engine">
		<aws>Amazon Elasticsearch Service</aws>
		<azure>Azure Search</azure>
		<gcp>Cloud Search</gcp>
	</service>
	
	<!--Application integration services-->
	<service name="Messaging">
		<aws>Amazon SNS</aws>
		<azure>Azure Event Grid</azure>
		<gcp>Cloud Pub/Sub</gcp>
	</service>
	<service name="Queueing">
		<aws>Amazon SQS</aws>
		<azure>Azure Service Bus</azure>
		<gcp>Cloud Pub/Sub</gcp>
	</service>
	
	<!--Monitoring services-->
	<service name="System Monitor">
		<aws>Amazon CloudWatch</aws>
		<azure>Azure Monitor</azure>
		<gcp>Stackdriver Monitoring</gcp>
	</service>
	<service name="User Monitor">
		<aws>AWS CloudTrail</aws>
		<azure>Azure Monitor Log Analytics</azure>
		<gcp>Stackdriver Logging</gcp>
	</service>
	
	<!--Network services-->
	<service name="VPC">
		<aws>Amazon VPC</aws>
		<azure>Azure Virtual Network</azure>
		<gcp>Google VPC</gcp>
	</service>
	<service name="VPN">
		<aws>AWS VPN</aws>
		<azure>Azure VPN Gateway</azure>
		<gcp>Google VPC</gcp>
	</service>
	<service name="API">
		<aws>Amazon API Gateway</aws>
		<azure>Azure API Management</azure>
		<gcp>Cloud Endpoints</gcp>
	</service>
	<service name="CDN">
		<aws>Amazon CloudFront</aws>
		<azure>Azure CDN</azure>
		<gcp>Cloud CDN</gcp>
	</service>
	<service name="DNS">
		<aws>Amazon Route 53</aws>
		<azure>Azure DNS</azure>
		<gcp>Cloud DNS</gcp>
	</service>
</services>